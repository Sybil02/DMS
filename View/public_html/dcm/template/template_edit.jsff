                      <af:commandButton text="删除" actionListener="#{bindings.DeleteParameter.execute}" id="cb21"/>
                      <af:commandButton text="保存" actionListener="#{bindings.Commit.execute}" id="cb22"/>
                      <af:commandButton text="取消" actionListener="#{bindings.Rollback.execute}" id="cb23">
