<?xml version='1.0' encoding='UTF-8'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
          xmlns:f="http://java.sun.com/jsf/core">
  <c:set value="#{adfBundle['dms_v']}" var="viewBundle"/>
  <af:panelStretchLayout id="psl1">
    <f:facet name="bottom">
    </f:facet>
    <f:facet name="center">
        <af:panelCollection id="pc1">
          <f:facet name="menus"/>
          <f:facet name="toolbar">
            <af:toolbar id="t1">
            <af:group id="g2">
              <af:commandButton text="#{viewBundle['dms.common.save']}" id="cb2"
                                action="#{viewScope.ProjectZxBean.operation_save}"/>
              <af:commandButton text="#{viewBundle['dms.common.cancel']}" id="cb3"
                                actionListener="#{viewScope.ProjectZxBean.reset}"/>
            </af:group>
            </af:toolbar>
          </f:facet>
          <f:facet name="statusbar"/>
        <af:table var="row" rowBandingInterval="0" id="t2"
                  value="#{viewScope.ProZzxOutBean.dataModel}"
                  selectionListener="#{viewScope.ProZzxOutBean.rowSelectionListener}"
                  selectedRowKeys="#{viewScope.ProZzxOutBean.dataModel.selectedRows}"
                  rowSelection="multiple" editingMode="clickToEdit"
                  emptyText="#{viewBundle['dms.common.nodata']}"
                  partialTriggers=":::soc2 :::soc1 :::soc3">
          <af:column sortable="false" headerText="WBS" id="c2">
            <af:inputText value="#{row[viewScope.ProZzxOutBean.dataModel.labelMap['WBS']]}" id="it26" readOnly="true"/>
          </af:column>
          <af:column sortable="false" headerText="作业活动" id="c5">
            <af:inputText value="#{row[viewScope.ProZzxOutBean.dataModel.labelMap['WORK']]}" id="it14" readOnly="true"/>
          </af:column>
          <af:column sortable="false" headerText="预算项" id="c19">
            <af:inputText value="#{row[viewScope.ProZzxOutBean.dataModel.labelMap['TERM']]}" id="it3" readOnly="true"/>
          </af:column>
          <af:column sortable="false" headerText="工作中心" id="c29">
            <af:inputText value="#{row[viewScope.ProZzxOutBean.dataModel.labelMap['CENTER']]}" id="it12" readOnly="true"/>
          </af:column>
          <af:column sortable="false" headerText="作业类型" id="c7">
            <af:inputText value="#{row[viewScope.ProZzxOutBean.dataModel.labelMap['WORK_TYPE']]}" id="it5" readOnly="true"/>
          </af:column>
          <af:column sortable="false" headerText="物料编码" id="c20">
            <af:inputText value="#{row[viewScope.ProZzxOutBean.dataModel.labelMap['BOM_CODE']]}" id="it28" readOnly="true"/>
          </af:column>
          <af:column sortable="false" headerText="单位" id="c14">
            <af:inputText value="#{row[viewScope.ProZzxOutBean.dataModel.labelMap['UNIT']]}" id="it27" readOnly="true"/>
          </af:column>
          <af:column sortable="false" headerText="计划成本" id="c1">
            <af:inputText value="#{row[viewScope.ProZzxOutBean.dataModel.labelMap['PLAN_COST']]}" id="it16" readOnly="true"/>
          </af:column>
        </af:table>
      </af:panelCollection>
    </f:facet>
    <f:facet name="start"/>
    <f:facet name="end"/>
    <f:facet name="top">
    <af:panelGroupLayout id="pgl1" layout="vertical" partialTriggers="soc1 soc2 soc3">
        <af:panelGroupLayout id="pgl2" layout="horizontal">
          <af:selectOneChoice value="#{viewScope.ProZzxOutBean.year}"
                              label="年" autoSubmit="true"
                              id="soc1"
                              valueChangeListener="#{viewScope.ProZzxOutBean.yearChange}">
            <f:selectItems value="#{viewScope.ProZzxOutBean.yearList}" id="si1"/>
          </af:selectOneChoice>
          <af:panelLabelAndMessage label="实体"
                                   id="plam1" >
            <af:outputText value="#{viewScope.ProZzxOutBean.entity}" id="ot1"/>
          </af:panelLabelAndMessage>
          <af:panelLabelAndMessage label="产品线"
                                   id="plam2">
            <af:outputText value="#{viewScope.ProZzxOutBean.PLine}" id="ot2"/>
          </af:panelLabelAndMessage>
          <af:selectOneChoice value="#{viewScope.ProZzxOutBean.pname}"
                              label="项目名称" autoSubmit="true"
                              id="soc2"
                              valueChangeListener="#{viewScope.ProZzxOutBean.projectChange}">
            <f:selectItems value="#{viewScope.ProZzxOutBean.pnameList}" id="si2"/>
          </af:selectOneChoice>
        </af:panelGroupLayout>
        <af:panelGroupLayout id="pgl3" layout="horizontal">
          <af:selectOneChoice value="#{viewScope.ProZzxOutBean.version}"
                              label="版本" autoSubmit="true"
                              id="soc3"
                              valueChangeListener="#{viewScope.ProZzxOutBean.versionChange}">
            <f:selectItems value="#{viewScope.ProZzxOutBean.versionList}" id="si3"/>
          </af:selectOneChoice>
          <af:panelLabelAndMessage label="项目类型"
                                   id="plam3">
            <af:outputText value="#{viewScope.ProZzxOutBean.proType}" id="ot3"/>
          </af:panelLabelAndMessage>
          <af:panelLabelAndMessage label="行业线"
                                   id="plam4">
            <af:outputText value="#{viewScope.ProZzxOutBean.HLine}"
                           id="ot4"/>
          </af:panelLabelAndMessage>
          <af:panelLabelAndMessage label="业务线"
                                   id="plam5">
            <af:outputText value="#{viewScope.ProZzxOutBean.YLine}"
                           id="ot5"/>
          </af:panelLabelAndMessage>
        </af:panelGroupLayout>
      </af:panelGroupLayout>
    </f:facet>
  </af:panelStretchLayout>
</jsp:root>
