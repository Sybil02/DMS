<?xml version='1.0' encoding='UTF-8'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1"
          xmlns:c="http://java.sun.com/jsp/jstl/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
          xmlns:f="http://java.sun.com/jsf/core">
  <c:set var="viewBundle" value="#{adfBundle['dms_v']}"/>
  <af:panelStretchLayout id="psl1" startWidth="0px" bottomHeight="1px">
    <f:facet name="bottom">
      <af:panelGroupLayout id="pgl4" layout="scroll"
                            xmlns:af="http://xmlns.oracle.com/adf/faces/rich">
        <af:popup id="p1" binding="#{viewScope.RPCostBean.errorWindow}">
          <af:panelWindow id="pw1" title="#{viewBundle['dcm.import_error']}">
            <af:panelStretchLayout id="psl2" inlineStyle="width:800px;height:400px">
              <f:facet name="bottom"/>
              <f:facet name="center">
                <af:table value="#{bindings.ProPlanCostView2.collectionModel}"
                          var="row"
                          rows="#{bindings.ProPlanCostView2.rangeSize}"
                          emptyText="#{bindings.ProPlanCostView2.viewable ? 'No data to display.' : 'Access Denied.'}"
                          fetchSize="#{bindings.ProPlanCostView2.rangeSize}"
                          rowBandingInterval="0"
                          selectedRowKeys="#{bindings.ProPlanCostView2.collectionModel.selectedRow}"
                          selectionListener="#{bindings.ProPlanCostView2.collectionModel.makeCurrent}"
                          rowSelection="single" id="t3"
                          columnStretching="column:c10">
                  <af:column sortProperty="#{bindings.ProPlanCostView2.hints.Network.name}"
                             sortable="false"
                             headerText="#{bindings.ProPlanCostView2.hints.Network.label}"
                             id="c8">
                    <af:outputText value="#{row.Network}" id="ot17"/>
                  </af:column>
                  <af:column sortProperty="#{bindings.ProPlanCostView2.hints.WorkCode.name}"
                             sortable="false"
                             headerText="#{bindings.ProPlanCostView2.hints.WorkCode.label}"
                             id="c3">
                    <af:outputText value="#{row.WorkCode}" id="ot7"/>
                  </af:column>
                  <af:column sortProperty="#{bindings.ProPlanCostView2.hints.Work.name}"
                             sortable="false"
                             headerText="#{bindings.ProPlanCostView2.hints.Work.label}"
                             id="c11">
                    <af:outputText value="#{row.Work}" id="ot8"/>
                  </af:column>
                  <af:column sortProperty="#{bindings.ProPlanCostView2.hints.Term.name}"
                             sortable="false"
                             headerText="#{bindings.ProPlanCostView2.hints.Term.label}"
                             id="c12">
                    <af:outputText value="#{row.Term}" id="ot12"/>
                  </af:column>
                  <af:column sortProperty="#{bindings.ProPlanCostView2.hints.CostDetail.name}"
                             sortable="false"
                             headerText="#{bindings.ProPlanCostView2.hints.CostDetail.label}"
                             id="c5">
                    <af:outputText value="#{row.CostDetail}" id="ot18"/>
                  </af:column>
                  <af:column sortProperty="#{bindings.ProPlanCostView2.hints.Center.name}"
                             sortable="false"
                             headerText="#{bindings.ProPlanCostView2.hints.Center.label}"
                             id="c9">
                    <af:outputText value="#{row.Center}" id="ot15"/>
                  </af:column>
                  <af:column sortProperty="#{bindings.ProPlanCostView2.hints.WorkType.name}"
                             sortable="false"
                             headerText="#{bindings.ProPlanCostView2.hints.WorkType.label}"
                             id="c6">
                    <af:outputText value="#{row.WorkType}" id="ot9"/>
                  </af:column>
                  <af:column sortProperty="#{bindings.ProPlanCostView2.hints.BomCode.name}"
                             sortable="false"
                             headerText="#{bindings.ProPlanCostView2.hints.BomCode.label}"
                             id="c2">
                    <af:outputText value="#{row.BomCode}" id="ot13"/>
                  </af:column>
                  <af:column sortProperty="#{bindings.ProPlanCostView2.hints.Msg.name}"
                             sortable="false"
                             headerText="#{bindings.ProPlanCostView2.hints.Msg.label}"
                             id="c10">
                    <af:outputText value="#{row.Msg}" id="ot19"/>
                  </af:column>
                  <af:column sortProperty="#{bindings.ProPlanCostView2.hints.CreatedAt.name}"
                             sortable="false"
                             headerText="#{bindings.ProPlanCostView2.hints.CreatedAt.label}"
                             id="c4">
                    <af:outputText value="#{row.CreatedAt}" id="ot11">
                      <af:convertDateTime pattern="#{bindings.ProPlanCostView2.hints.CreatedAt.format}"/>
                    </af:outputText>
                  </af:column>
                  <af:column sortProperty="#{bindings.ProPlanCostView2.hints.CreatedBy.name}"
                             sortable="false"
                             headerText="#{bindings.ProPlanCostView2.hints.CreatedBy.label}"
                             id="c7">
                    <af:outputText value="#{row.CreatedBy}" id="ot16"/>
                  </af:column>
                </af:table>
              </f:facet>
              <f:facet name="start"/>
              <f:facet name="end"/>
              <f:facet name="top"/>
            </af:panelStretchLayout>
          </af:panelWindow>
        </af:popup>
        <af:popup id="p2" autoCancel="disabled" childCreation="deferred"
                  binding="#{viewScope.RPCostBean.dataExportWnd}">
          <af:panelWindow id="pw2" title="#{viewBundle['dms.dgpt.dataExport']}">
            <af:panelGridLayout id="pgl5">
              <af:gridRow marginTop="5px" height="auto" id="gr2">
                <af:gridCell marginStart="5px" width="auto" id="gc6"
                             inlineStyle="padding-top:5px">
                  <af:outputText value="#{viewBundle['dms.dgpt.excel.format']}"
                                 id="ot10"/>
                </af:gridCell>
                <af:gridCell marginStart="5px" marginEnd="5px" width="auto"
                             id="gc9">
                 <!-- <af:selectBooleanRadio text="#{viewBundle['dms.dgpt.excel.03format']}"
                                         id="sbr3" group="excelFormat1"
                                         simple="true"/>-->
                  <af:selectBooleanRadio text="#{viewBundle['dms.dgpt.excel.07format']}"
                                         id="sbr4" group="excelFormat1"
                                         simple="true" autoSubmit="true"
                                         value="#{viewScope.RPCostBean.isXlsx}"/>
                </af:gridCell>
              </af:gridRow>
              <af:gridRow marginTop="5px" marginBottom="5px" height="auto"
                          id="gr1">
                <af:gridCell marginStart="5px" width="auto" columnSpan="2"
                             halign="center" id="gc8"
                             inlineStyle="margin-top:5px">
                  <af:commandButton text="#{viewBundle['dcm.action.export']}"
                                    id="cb11">
                    <af:fileDownloadActionListener contentType="application/vnd.ms-excel"
                                                   method="#{viewScope.RPCostBean.operation_export}"
                                                   filename="#{viewScope.RPCostBean.exportDataExcelName}"/>
                  </af:commandButton>
                </af:gridCell>
              </af:gridRow>
            </af:panelGridLayout>
          </af:panelWindow>
        </af:popup>
        <af:popup id="p3" binding="#{viewScope.RPCostBean.dataImportWnd}">
          <af:panelWindow id="pw3"
                          title="#{viewBundle['dgpt.template.data_import']}">
            <af:panelGridLayout id="pgl6">
              <af:gridRow marginTop="5px" height="auto" id="gr6">
                <af:gridCell marginStart="5px" width="auto" id="gc2">
                  <af:outputText value="#{viewBundle['dcm.plz_select_import_file']}" id="ot14"/>
                </af:gridCell>
                <af:gridCell marginStart="5px" marginEnd="5px" width="auto"
                             id="gc1">
                  <af:inputFile id="if1" simple="true"
                                binding="#{viewScope.RPCostBean.fileInput}"/>
                </af:gridCell>
              </af:gridRow>
             <!-- <af:gridRow marginTop="5px" height="auto" id="gr5">
                <af:gridCell marginStart="5px" width="auto" id="gc11"
                             marginEnd="5px">
                  <af:outputText value="#{viewBundle['dcm.template.import_type_msg']}" id="ot15"/>
                </af:gridCell>
                <af:gridCell marginStart="5px" marginEnd="5px" width="auto"
                             id="gc4">
                  <af:selectBooleanRadio text="#{viewBundle['dcm.import.increment']}" id="sbr1"
                                         group="rpImport" simple="true"/>
                  <af:selectBooleanRadio text="#{viewBundle['dcm.import.repalce']}" id="sbr2"
                                         group="rpImport" simple="true"/>
                </af:gridCell>
              </af:gridRow>-->
              <af:gridRow marginTop="5px" height="auto" id="gr3">
                <af:gridCell marginStart="5px" width="50%" id="gc7"/>
                <af:gridCell marginStart="5px" marginEnd="5px" width="50%"
                             id="gc3"/>
              </af:gridRow>
              <af:gridRow marginTop="5px" marginBottom="5px" height="auto"
                          id="gr4">
                <af:gridCell marginStart="5px" width="auto" id="gc5"
                             halign="center">
                  <af:commandButton text="#{viewBundle['dms.dgpt.excel.import']}" id="cb8"
                                    actionListener="#{viewScope.RPCostBean.operation_import}"/>
                </af:gridCell>
              </af:gridRow>
            </af:panelGridLayout>
          </af:panelWindow>
        </af:popup>
        <af:popup id="p4" binding="#{viewScope.RPCostBean.adminBlockPop}" autoCancel="disabled">
          <af:panelWindow id="pw4"
                          title="#{viewBundle['dms.dgpt.block_status.select']}">
            <af:panelCollection id="pc2" inlineStyle="width:800px;height:400px">
              <f:facet name="menus"/>
              <f:facet name="toolbar">
              <af:toolbar id="t6">
                  <af:group id="g2">
                    <af:commandButton text="#{viewBundle['dms.dgpt.fblock']}" id="cb10"
                                      actionListener="#{viewScope.RPCostBean.goUnBlock}"/>
                  </af:group>
                </af:toolbar>
              </f:facet>
              <f:facet name="statusbar"/>
              <af:table value="#{bindings.adminBlockStatus.collectionModel}"
                        var="row" rows="#{bindings.adminBlockStatus.rangeSize}"
                        emptyText="#{bindings.adminBlockStatus.viewable ? 'No data to display.' : 'Access Denied.'}"
                        fetchSize="#{bindings.adminBlockStatus.rangeSize}"
                        rowBandingInterval="0"
                        filterModel="#{bindings.adminBlockStatusQuery.queryDescriptor}"
                        queryListener="#{bindings.adminBlockStatusQuery.processQuery}"
                        filterVisible="false" varStatus="vs"
                        selectionListener="#{bindings.adminBlockStatus.collectionModel.makeCurrent}"
                        rowSelection="multiple" id="t4"
                        binding="#{viewScope.RPCostBean.subTable}"
                        columnStretching="column:c16">
                <af:column sortProperty="#{bindings.adminBlockStatus.hints.Meaning.name}"
                           filterable="true" sortable="false"
                           headerText="#{bindings.adminBlockStatus.hints.Meaning.label}"
                           id="c14">
                  <af:outputText value="#{row.Meaning}" id="ot21"/>
                </af:column>
                <af:column sortProperty="#{bindings.adminBlockStatus.hints.ProjectName.name}"
                           filterable="true" sortable="false"
                           headerText="#{bindings.adminBlockStatus.hints.ProjectName.label}"
                           id="c16">
                  <af:outputText value="#{row.ProjectName}" id="ot22"/>
                </af:column>
                <af:column sortProperty="#{bindings.adminBlockStatus.hints.Version.name}"
                           filterable="true" sortable="false"
                           headerText="#{bindings.adminBlockStatus.hints.Version.label}"
                           id="c15">
                  <af:outputText value="#{row.Version}" id="ot20"/>
                </af:column>
                <af:column sortProperty="#{bindings.adminBlockStatus.hints.IsBlock.name}"
                           filterable="true" sortable="false"
                           headerText="#{bindings.adminBlockStatus.hints.IsBlock.label}"
                           id="c13">
                  <af:outputText value="#{row.IsBlock}" id="ot23"/>
                </af:column>
              </af:table>
            </af:panelCollection>
          </af:panelWindow>
        </af:popup>
        <af:popup id="p5" binding="#{viewScope.RPCostBean.statusWindow}" autoCancel="disabled">
          <af:panelWindow id="pw5"
                          title="#{viewBundle['dms.dgpt.block_status.select']}">
            <af:panelCollection id="pc3" inlineStyle="width:800px;height:400px">
              <f:facet name="menus"/>
              <f:facet name="toolbar">
                <af:toolbar id="t7">
                  <af:group id="g4">
                    <af:commandButton text="#{viewBundle['dms.dgpt.fblock']}" id="cb12"
                                      rendered="#{sessionScope.authoriedFunction['UNFREEZE']}"
                                      actionListener="#{viewScope.RPCostBean.goUnBlock2}"/>
                  </af:group>
                </af:toolbar>
              </f:facet>
              <f:facet name="statusbar"/>
              <af:table value="#{bindings.PTBlockStatus.collectionModel}"
                        var="row" rows="#{bindings.PTBlockStatus.rangeSize}"
                        emptyText="#{bindings.PTBlockStatus.viewable ? 'No data to display.' : 'Access Denied.'}"
                        fetchSize="#{bindings.PTBlockStatus.rangeSize}"
                        rowBandingInterval="0"
                        filterModel="#{bindings.PTBlockStatusQuery.queryDescriptor}"
                        queryListener="#{bindings.PTBlockStatusQuery.processQuery}"
                        filterVisible="false" varStatus="vs"
                        selectionListener="#{bindings.PTBlockStatus.collectionModel.makeCurrent}"
                        rowSelection="multiple" id="t5"
                        columnStretching="column:c18"
                        binding="#{viewScope.RPCostBean.subTable2}">
                <af:column sortProperty="#{bindings.PTBlockStatus.hints.Meaning.name}"
                           filterable="true" sortable="false"
                           headerText="#{bindings.PTBlockStatus.hints.Meaning.label}"
                           id="c20">
                  <af:outputText value="#{row.Meaning}" id="ot27"/>
                </af:column>
                <af:column sortProperty="#{bindings.PTBlockStatus.hints.ProjectName.name}"
                           filterable="true" sortable="false"
                           headerText="#{bindings.PTBlockStatus.hints.ProjectName.label}"
                           id="c18">
                  <af:outputText value="#{row.ProjectName}" id="ot25"/>
                </af:column>
                <af:column sortProperty="#{bindings.PTBlockStatus.hints.Version.name}"
                           filterable="true" sortable="false"
                           headerText="#{bindings.PTBlockStatus.hints.Version.label}"
                           id="c17">
                  <af:outputText value="#{row.Version}" id="ot26"/>
                </af:column>
                <af:column sortProperty="#{bindings.PTBlockStatus.hints.IsBlock.name}"
                           filterable="true" sortable="false"
                           headerText="#{bindings.PTBlockStatus.hints.IsBlock.label}"
                           id="c19">
                  <af:outputText value="#{row.IsBlock}" id="ot24"/>
                </af:column>
              </af:table>
            </af:panelCollection>
          </af:panelWindow>
        </af:popup>
      </af:panelGroupLayout>
    </f:facet>
    <f:facet name="center">
      <af:panelCollection id="pc1">
        <f:facet name="menus"/>
        <f:facet name="toolbar">
          <af:toolbar id="t1">
            <af:group id="g1">
              <af:commandButton text="#{viewBundle['dms.common.save']}" id="cb1"
                                actionListener="#{viewScope.RPCostBean.operation_save}"
                                partialTriggers="::soc1 ::iclov2 ::soc3"
                                disabled="#{viewScope.RPCostBean.isSelected or not viewScope.RPCostBean.isEDITABLE or viewScope.RPCostBean.isBlock}"/>
              <af:commandButton text="#{viewBundle['dms.common.cancel']}" id="cb2"
                                actionListener="#{viewScope.RPCostBean.operation_reset}"/>
            </af:group>
            <af:group id="g3">
              <af:commandButton text="#{viewBundle['dms.dgpt.excel.import']}"
                                id="cb6" disabled="#{viewScope.RPCostBean.isBlock or not viewScope.RPCostBean.isEDITABLE or viewScope.RPCostBean.isSelected}"
                                partialTriggers="cb3">
                   <af:showPopupBehavior popupId="::p3"/>             
             </af:commandButton>
              <af:commandButton text="#{viewBundle['dcm.action.export']}" id="cb7"
                                partialTriggers="::soc1 ::iclov2 ::soc3"
                                disabled="#{viewScope.RPCostBean.isSelected}">
                <af:showPopupBehavior popupId="::p2"/>
              </af:commandButton>
              <af:commandButton text="#{viewBundle['dms.dgpt.block']}" id="cb3"
                                actionListener="#{viewScope.RPCostBean.beBlocked}"
                                     disabled="#{viewScope.RPCostBean.isBlock or not viewScope.RPCostBean.isEDITABLE}"
                                partialTriggers="::soc1 ::iclov2 ::soc3"/>
              <af:commandButton text="#{viewBundle['dms.dgpt.block.status']}" id="cb9"
                                actionListener="#{viewScope.RPCostBean.blockStatusPop}"/>
              <af:commandButton text="#{viewBundle['dms.dgpt.fblock']}" id="cb5"
                                rendered="#{sessionScope.authoriedFunction['UNFREEZE'] or viewScope.RPCostBean.isManager}"
                                actionListener="#{viewScope.RPCostBean.outBlock}"
                                disabled="#{not viewScope.RPCostBean.isBlock}"
                                partialTriggers="::soc1 ::iclov2 ::soc3"/>
              <af:commandButton text="#{viewBundle['dcm.import_error']}" id="cb4"
                                actionListener="#{viewScope.RPCostBean.errorPop}"/>
            </af:group>
          </af:toolbar>
        </f:facet>
        <f:facet name="statusbar"/>
        <af:table var="row" rowBandingInterval="0" id="t2"
                  value="#{viewScope.RPCostBean.dataModel}"
                  selectionListener="#{viewScope.RPCostBean.rowSelectionListener}"
                  selectedRowKeys="#{viewScope.RPCostBean.dataModel.selectedRows}"
                  rowSelection="multiple" editingMode="clickToEdit"
                  emptyText="#{viewBundle['dms.common.nodata']}"
                  partialTriggers=":::soc1 :::iclov2 :::soc3 ::cb3 ::cb5">
            <af:forEach items="#{viewScope.RPCostBean.dataModel.pcColsDef}"
                        var="attr" varStatus="vs">
                  <af:column sortable="false" headerText="#{attr.columnLable}" id="c1"
                            inlineStyle="#{'UPDATE' eq row['OPERATION'] ? 'background-color:GREEN;' : ''}
                                    #{'CREATE' eq row['OPERATION'] ? 'background-color:RED;' : ''}
                                    #{'DELETE' eq row['OPERATION'] ? 'background-color:YELLOW;' : ''}"
                             rendered="#{attr.dbTableCol ne &quot;ROW_ID&quot;}">
                    <af:inputText value="#{row[attr.dbTableCol]}" id="ot6"
                                  readOnly="#{attr.isReadonly eq 'false' ? 
                                  viewScope.RPCostBean.isBlock or not viewScope.RPCostBean.isEDITABLE : attr.isReadonly}"
                                    valueChangeListener="#{viewScope.RPCostBean.valueChangeLinstener}"/>
                  </af:column>
          </af:forEach>
        </af:table>
      </af:panelCollection>
    </f:facet>
    <f:facet name="start"/>
    <f:facet name="end"/>
    <f:facet name="top">
      <af:panelGroupLayout id="pgl1" layout="vertical" >
        <af:panelGroupLayout id="pgl2" layout="horizontal">
        <af:selectOneChoice value="#{viewScope.RPCostBean.year}"
                              label="年" autoSubmit="true"
                              id="soc1"
                              valueChangeListener="#{viewScope.RPCostBean.yearChange}">
            <f:selectItems value="#{viewScope.RPCostBean.yearList}" id="si1"/>
          </af:selectOneChoice>
          <!--af:selectOneChoice value="#{viewScope.RPCostBean.pname}"
                              label="项目名称" autoSubmit="true"
                              id="iclov2"
                              valueChangeListener="#{viewScope.RPCostBean.projectChange}">
            <f:selectItems value="#{viewScope.RPCostBean.pnameList}" id="si2"/>
          </af:selectOneChoice-->
                    <af:inputComboboxListOfValues label="项目名称" inlineStyle="margin-right:5px"
                                                value="#{viewScope.RPCostBean.pname}"
                                                autoSubmit="true" clientComponent="true"
                                                validator="#{viewScope.RPCostBean.proLov.listOfValuesModel.validate}"
                                                model="#{viewScope.RPCostBean.proLov.listOfValuesModel}"
                                                id="iclov2" columns="80"
                                                valueChangeListener="#{viewScope.RPCostBean.projectChange}"
                                              usage="auto">
                    <af:autoSuggestBehavior suggestedItems="#{viewScope.RPCostBean.proLov.listOfValuesModel.suggest}"/>
          </af:inputComboboxListOfValues>
          <af:selectOneChoice value="#{viewScope.RPCostBean.version}"
                              label="版本" autoSubmit="true"
                              id="soc3"
                              valueChangeListener="#{viewScope.RPCostBean.versionChange}"
                              partialTriggers="iclov2">
            <f:selectItems value="#{viewScope.RPCostBean.versionList}" id="si3"/>
          </af:selectOneChoice>
          <af:panelLabelAndMessage label="产品线"
                                   id="plam2">
            <af:outputText value="#{viewScope.RPCostBean.PLine}" partialTriggers="soc1 iclov2 soc3" id="ot2"/>
          </af:panelLabelAndMessage>
        </af:panelGroupLayout>
        <af:panelGroupLayout id="pgl3" layout="horizontal">
            <af:panelLabelAndMessage label="实体"
                                   id="plam1" >
            <af:outputText value="#{viewScope.RPCostBean.entity}" partialTriggers="soc1 iclov2 soc3" id="ot1"/>
          </af:panelLabelAndMessage>
          <af:panelLabelAndMessage label="行业线"
                                   id="plam4">
            <af:outputText value="#{viewScope.RPCostBean.HLine}" partialTriggers="soc1 iclov2 soc3"
                           id="ot4"/>
          </af:panelLabelAndMessage>
          <af:panelLabelAndMessage label="业务线"
                                   id="plam5">
            <af:outputText value="#{viewScope.RPCostBean.YLine}" partialTriggers="soc1 iclov2 soc3"
                           id="ot5"/>
          </af:panelLabelAndMessage>
          <af:panelLabelAndMessage label="项目类型"
                                   id="plam3">
            <af:outputText value="#{viewScope.RPCostBean.proType}" partialTriggers="soc1 iclov2 soc3" id="ot3"/>
          </af:panelLabelAndMessage>
        </af:panelGroupLayout>
      </af:panelGroupLayout>
    </f:facet>
  </af:panelStretchLayout>
</jsp:root>
