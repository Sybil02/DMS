<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel"
                version="11.1.1.64.93" id="data_displayPageDef" Package="dcm">
  <parameters/>
  <executables>
    <variableIterator id="variables"/>
    <iterator id="DcmErrorViewIterator" Binds="DcmErrorView"
              DataControl="DcmModuleDataControl" RangeSize="25"
              CacheResults="true"/>
    <iterator Binds="DcmErrorBatchVO" RangeSize="-1"
              DataControl="DcmModuleDataControl" id="DcmErrorBatchVOIterator"/>
    <searchRegion Binds="DcmErrorBatchVOIterator" Criteria=""
                  Customizer="oracle.jbo.uicli.binding.JUSearchBindingCustomizer"
                  id="DcmErrorBatchVOQuery"/>
    <methodIterator Binds="getSubmitStatusVO.result"
                    DataControl="DcmModuleDataControl" RangeSize="25"
                    id="getSubmitStatusVOIterator"/>
  </executables>
  <bindings>
    <tree IterBinding="DcmErrorViewIterator" id="DcmErrorView">
      <nodeDefinition DefName="team.epm.dcm.view.DcmErrorView"
                      Name="DcmErrorView0">
        <AttrNames>
          <Item Value="SheetName"/>
          <Item Value="RowNum"/>
          <Item Value="Msg"/>
          <Item Value="Level"/>
          <Item Value="CreatedBy"/>
          <Item Value="CreatedAt"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <tree IterBinding="DcmErrorBatchVOIterator" id="DcmErrorBatchVO">
      <nodeDefinition DefName="team.epm.dcm.view.DcmErrorBatchVO"
                      Name="DcmErrorBatchVO0">
        <AttrNames>
          <Item Value="SheetName"/>
          <Item Value="Status"/>
          <Item Value="Line"/>
          <Item Value="Msg"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <methodAction id="getSubmitStatusVO" RequiresUpdateModel="true"
                  Action="invokeMethod" MethodName="getSubmitStatusVO"
                  IsViewObjectMethod="false" DataControl="DcmModuleDataControl"
                  InstanceName="DcmModuleDataControl.dataProvider"
                  ReturnName="data.DcmModuleDataControl.methodResults.getSubmitStatusVO_DcmModuleDataControl_dataProvider_getSubmitStatusVO_result">
      <NamedData NDName="sql" NDValue="#{viewScope.DcmDataDisplayBean.subSql}"
                 NDType="java.lang.String"/>
      <NamedData NDName="templateId"
                 NDValue="#{viewScope.DcmDataDisplayBean.subTempId}"
                 NDType="java.lang.String"/>
      <NamedData NDName="userId" NDValue="#{sessionScope.cur_user.id}"
                 NDType="java.lang.String"/>
    </methodAction>
    <tree IterBinding="getSubmitStatusVOIterator" id="return">
      <nodeDefinition Name="return0"/>
    </tree>
  </bindings>
</pageDefinition>
