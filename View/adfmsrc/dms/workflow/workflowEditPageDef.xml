<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel"
                version="11.1.1.64.93" id="workflowEditPageDef"
                Package="dms.workflow">
  <parameters/>
  <executables>
    <variableIterator id="variables"/>
    <iterator Binds="DmsWorkflowInfoVO" RangeSize="25"
              DataControl="DmsModuleDataControl"
              id="DmsWorkflowInfoVOIterator"/>
    <iterator Binds="DmsWorkflowStepsVO" RangeSize="25"
              DataControl="DmsModuleDataControl"
              id="DmsWorkflowStepsVOIterator"/>
    <iterator Binds="DmsWorkflowStepProVO" RangeSize="25"
              DataControl="DmsModuleDataControl"
              id="DmsWorkflowStepProVOIterator"/>
  </executables>
  <bindings>
    <tree IterBinding="DmsWorkflowInfoVOIterator" id="DmsWorkflowInfoVO">
      <nodeDefinition DefName="team.epm.dms.view.DmsWorkflowInfoVO"
                      Name="DmsWorkflowInfoVO0">
        <AttrNames>
          <Item Value="WfName"/>
          <Item Value="WfCom"/>
          <Item Value="Seq"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <tree IterBinding="DmsWorkflowStepsVOIterator" id="DmsWorkflowStepsVO">
      <nodeDefinition DefName="team.epm.dms.view.DmsWorkflowStepsVO"
                      Name="DmsWorkflowStepsVO0">
        <AttrNames>
          <Item Value="StepNo"/>
          <Item Value="StepTask"/>
          <Item Value="PreStep"/>
          <Item Value="LabelObject"/>
          <Item Value="ApproveObject"/>
          <Item Value="EtlObject"/>
          <Item Value="AfterPro"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <action IterBinding="DmsWorkflowInfoVOIterator" id="WorkflowCreateInsert"
            InstanceName="DmsModuleDataControl.DmsWorkflowInfoVO"
            DataControl="DmsModuleDataControl" RequiresUpdateModel="true"
            Action="createInsertRow"/>
    <action IterBinding="DmsWorkflowInfoVOIterator" id="WorkflowDelete"
            InstanceName="DmsModuleDataControl.DmsWorkflowInfoVO"
            DataControl="DmsModuleDataControl" RequiresUpdateModel="false"
            Action="removeCurrentRow"/>
    <action IterBinding="DmsWorkflowStepsVOIterator" id="StepsCreateInsert"
            InstanceName="DmsModuleDataControl.DmsWorkflowStepsVO"
            DataControl="DmsModuleDataControl" RequiresUpdateModel="true"
            Action="createInsertRow"/>
    <action IterBinding="DmsWorkflowStepsVOIterator" id="StepsDelete"
            InstanceName="DmsModuleDataControl.DmsWorkflowStepsVO"
            DataControl="DmsModuleDataControl" RequiresUpdateModel="false"
            Action="removeCurrentRow"/>
    <action id="Commit" DataControl="DmsModuleDataControl"
            RequiresUpdateModel="true" Action="commitTransaction"/>
    <action id="Rollback" DataControl="DmsModuleDataControl"
            RequiresUpdateModel="false" Action="rollbackTransaction"/>
    <tree IterBinding="DmsWorkflowStepProVOIterator" id="DmsWorkflowStepProVO">
      <nodeDefinition DefName="team.epm.dms.view.DmsWorkflowStepProVO"
                      Name="DmsWorkflowStepProVO0">
        <AttrNames>
          <Item Value="CalcName"/>
          <Item Value="CalcPro"/>
          <Item Value="Seq"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <action IterBinding="DmsWorkflowStepProVOIterator" id="ProCreateInsert"
            InstanceName="DmsModuleDataControl.DmsWorkflowStepProVO"
            DataControl="DmsModuleDataControl" RequiresUpdateModel="true"
            Action="createInsertRow"/>
    <action IterBinding="DmsWorkflowStepProVOIterator" id="ProDelete"
            InstanceName="DmsModuleDataControl.DmsWorkflowStepProVO"
            DataControl="DmsModuleDataControl" RequiresUpdateModel="false"
            Action="removeCurrentRow"/>
  </bindings>
</pageDefinition>
