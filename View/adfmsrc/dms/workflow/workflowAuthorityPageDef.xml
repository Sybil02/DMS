<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel"
                version="11.1.1.64.93" id="workflowAuthorityPageDef"
                Package="dms.workflow">
  <parameters/>
  <executables>
    <variableIterator id="variables"/>
    <iterator Binds="DmsEnabledRole" RangeSize="-1"
              DataControl="DmsModuleDataControl" id="DmsEnabledRoleIterator"/>
    <iterator Binds="DmsSetWorkflowInfoVO" RangeSize="-1"
              DataControl="DmsModuleDataControl"
              id="DmsSetWorkflowInfoVOIterator"/>
    <iterator Binds="DmsRoleWorkflowVO" RangeSize="-1"
              DataControl="DmsModuleDataControl"
              id="DmsRoleWorkflowVOIterator"/>
  </executables>
  <bindings>
    <list ListOperMode="navigation" IterBinding="DmsEnabledRoleIterator"
          ListIter="DmsEnabledRoleIterator" id="RoleName" DTSupportsMRU="true">
      <AttrNames>
        <Item Value="RoleName"/>
      </AttrNames>
    </list>
    <action id="Rollback" DataControl="DmsModuleDataControl"
            RequiresUpdateModel="false" Action="rollbackTransaction"/>
    <action id="Commit" DataControl="DmsModuleDataControl"
            RequiresUpdateModel="true" Action="commitTransaction"/>
    <tree IterBinding="DmsSetWorkflowInfoVOIterator" id="DmsSetWorkflowInfoVO">
      <nodeDefinition DefName="team.epm.dms.view.DmsSetWorkflowInfoVO"
                      Name="DmsSetWorkflowInfoVO0">
        <AttrNames>
          <Item Value="WfName"/>
          <Item Value="WfStatus"/>
          <Item Value="UpdatedAt"/>
          <Item Value="UpdatedBy"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <tree IterBinding="DmsRoleWorkflowVOIterator" id="DmsRoleWorkflowVO">
      <nodeDefinition DefName="team.epm.dms.view.DmsRoleWorkflowVO"
                      Name="DmsRoleWorkflowVO0">
        <AttrNames>
          <Item Value="Id"/>
          <Item Value="WfName"/>
          <Item Value="RoleId"/>
          <Item Value="UpdatedAt"/>
          <Item Value="EnableRun"/>
        </AttrNames>
      </nodeDefinition>
    </tree>
    <list IterBinding="DmsRoleWorkflowVOIterator" id="WorkflowId"
          DTSupportsMRU="true" StaticList="false"
          ListIter="DmsSetWorkflowInfoVOIterator">
      <AttrNames>
        <Item Value="WorkflowId"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="Id"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="WfName"/>
      </ListDisplayAttrNames>
    </list>
  </bindings>
</pageDefinition>
