<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="DcmUserTemplateView"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="team.epm.dcm.view.DcmUserTemplateView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="locale"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[adf.context.locale]]></TransientExpression>
  </Variable>
  <Variable
    Name="userId"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[adf.userSession.userData.userId]]></TransientExpression>
  </Variable>
  <SQLQuery>
    <![CDATA[SELECT T5.ID "TEMPLATE_ID", T5.NAME "TEMPLATE_NAME",T4.READ_ONLY
  FROM DMS_USER          T1,
       DMS_USER_GROUP    T2,
       DMS_GROUP_ROLE    T3,
       DCM_ROLE_TEMPLATE T4,
       DCM_TEMPLATE      T5,
       DMS_GROUP         T6,
       DMS_ROLE          T7
 WHERE T1.ID = T2.USER_ID
   AND T2.GROUP_ID = T3.GROUP_ID
   AND T2.GROUP_ID = T6.ID
   AND T6.ENABLE_FLAG = 'Y'
   AND T6.LOCALE = T5.LOCALE
   AND T3.ROLE_ID = T4.ROLE_ID
   AND T3.ROLE_ID = T7.ID
   AND T7.ENABLE_FLAG = 'Y'
   AND T7.LOCALE = T6.LOCALE
   AND T4.Template_Id = T5.ID
   AND T5.LOCALE = :locale
   AND T1.ID=:userId]]>
  </SQLQuery>
  <ViewAttribute
    Name="TemplateId"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="TEMPLATE_ID"
    Expression="TEMPLATE_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="32"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="TemplateName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="300"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="TEMPLATE_NAME"
    Expression="TEMPLATE_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="300"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ReadOnly"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="10"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="READ_ONLY"
    Expression="READ_ONLY"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="10"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="ModelBundle"/>
  </ResourceBundle>
</ViewObject>
