<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="DmsUnAssignedAppVO"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="team.epm.dms.view.DmsUnAssignedAppVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="roleId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[SELECT T.ID,
       T.APPROVAL_NAME,
       T.TEMPLATE_ID,
       T.VALUE_SET_ID,
       T.SEQ
FROM DMS_APPROVALFLOW_INFO T
WHERE NOT EXISTS (SELECT 1 
                       FROM DMS_ROLE_APPROVAL D 
                       WHERE D.ROLE_ID=:roleId 
                       AND D.APPROVAL_ID=T.ID)
ORDER BY T.SEQ]]>
  </SQLQuery>
  <ViewAttribute
    Name="Id"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="ID"
    Expression="ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="32"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ApprovalName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="APPROVAL_NAME"
    Expression="APPROVAL_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="dms.approval.name"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="TemplateId"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="32"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="TEMPLATE_ID"
    Expression="TEMPLATE_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="32"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="ValueSetId"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="32"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="VALUE_SET_ID"
    Expression="VALUE_SET_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="32"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="Seq"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="10"
    Scale="0"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="SEQ"
    Expression="SEQ"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="11"/>
