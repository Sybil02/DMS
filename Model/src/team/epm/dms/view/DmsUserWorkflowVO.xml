<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="DmsUserWorkflowVO"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="team.epm.dms.view.DmsUserWorkflowVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="locale"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[adf.context.locale]]></TransientExpression>
  </Variable>
  <Variable
    Name="userid"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[adf.userSession.userData.userId]]></TransientExpression>
  </Variable>
  <SQLQuery>
    <![CDATA[SELECT T5.ID "WORKFLOW_ID", T5.WF_NAME "WF_NAME" ,T5.WF_STATUS,T5.WF_COM
  FROM DMS_USER          T1,
       DMS_USER_GROUP    T2,
       DMS_GROUP_ROLE    T3,
       DMS_ROLE_WORKFLOW T4,
       DMS_WORKFLOWINFO  T5,
       DMS_GROUP         T6,
       DMS_ROLE          T7
 WHERE T1.ID = T2.USER_ID
   AND T2.GROUP_ID = T3.GROUP_ID
   AND T2.GROUP_ID = T6.ID
   AND T6.ENABLE_FLAG = 'Y'
   AND T6.LOCALE = T5.LOCALE
   AND T3.ROLE_ID = T4.ROLE_ID
   AND T3.ROLE_ID = T7.ID
   AND T7.ENABLE_FLAG = 'Y'
   AND T7.LOCALE = T6.LOCALE
   AND T4.WORKFLOW_ID = T5.ID
   AND T5.LOCALE =: locale
   AND T1.ID = : userid
   ORDER BY T5.SEQ]]>
  </SQLQuery>
  <ViewAttribute
    Name="WorkflowId"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="WORKFLOW_ID"
    Expression="WORKFLOW_ID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="32"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="WfName"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="WF_NAME"
    Expression="WF_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="team.epm.dms.view.DmsUserWorkflowVO.WfName_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="WfStatus"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="1"
    Type="java.lang.String"
    ColumnType="CHAR"
    AliasName="WF_STATUS"
    Expression="WF_STATUS"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="1"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="WfCom"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="32"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="WF_COM"
    Expression="WF_COM"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="32"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="ModelBundle"/>
  </ResourceBundle>
</ViewObject>
