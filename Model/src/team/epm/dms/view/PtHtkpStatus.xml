    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="80"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="VERSION"
    Expression="VERSION"
    SQLType="VARCHAR">
    <DesignTime>
