<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="PtHtkpStatus"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="team.epm.dms.view.PtHtkpStatus_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="userId"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="userAcc"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[SELECT  C.ID,Y.MEANING AS YEAR,B.MEANING AS PRO_NAME,V.MEANING AS VERSION,'已冻结' AS IS_BLOCK
  FROM DCM_COMBINATION_17 C, HLS_HTKP_PROJECT_V B,HLS_YEAR_C Y,HLS_VERSION_C V
 WHERE B.MEANING IN (SELECT T.PRO_CODE || '-' || T.PRO_DESC
                       FROM SAP_DMS_PROJECT_PRIVILEGE T
                      WHERE T.ATTRIBUTE7 = :userAcc
                         OR T.ATTRIBUTE5 IN
                            (SELECT G.GROUP_ID
                               FROM DMS_USER_GROUP G
                              WHERE G.USER_ID = :userId))
   AND C.ID IN (SELECT COM_RECORD_ID FROM DMS_HTKP_BLOCK)
   AND C.ID IN (SELECT COM_RECORD_ID FROM CONT_INVOICE_RETURN_BUDGET_5)                            
   AND C.BH_USER_PRO_C1 = B.CODE
   AND C.HLS_YEAR_C = Y.CODE
   AND C.HLS_VERSION_C = V.CODE
UNION   
SELECT  C.ID,Y.MEANING AS YEAR,B.MEANING AS PRO_NAME,V.MEANING AS VERSION,'未冻结' AS IS_BLOCK
  FROM DCM_COMBINATION_17 C, HLS_HTKP_PROJECT_V B,HLS_YEAR_C Y,HLS_VERSION_C V
 WHERE B.MEANING IN (SELECT T.PRO_CODE || '-' || T.PRO_DESC
                       FROM SAP_DMS_PROJECT_PRIVILEGE T
                      WHERE T.ATTRIBUTE7 = :userAcc
                         OR T.ATTRIBUTE5 IN
                            (SELECT G.GROUP_ID
                               FROM DMS_USER_GROUP G
                              WHERE G.USER_ID = :userId))
   AND C.ID NOT IN (SELECT COM_RECORD_ID FROM DMS_HTKP_BLOCK)
   AND C.ID IN (SELECT COM_RECORD_ID FROM CONT_INVOICE_RETURN_BUDGET_5)                             
   AND C.BH_USER_PRO_C1 = B.CODE
   AND C.HLS_YEAR_C = Y.CODE
   AND C.HLS_VERSION_C = V.CODE]]>
  </SQLQuery>
  <ViewAttribute
