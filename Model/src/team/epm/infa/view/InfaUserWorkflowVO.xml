  </Variable>
  <Variable
    Name="catId"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[SELECT INFA.ID,
       INFA.CAT_ID,
       INFA.REP_ID,
       INFA.FOLDERNAME,
       INFA.WORKFLOW_NAME,
       INFA.REQUEST_MODE,
       INFA.SERVICE_NAME,
       SV.PROTOCOL,
       SV.SERVER_HOST,
       SV.SERVER_PORT,
       RP.REP_DOMAIN,
       RP.REP_NAME,
       RP.USER_NAME,
       RP.PASSWORD,
       INFA.LOCALE,
       INFA.IDX
  FROM INFA_WORKFLOW INFA,INFA_SERVER_INFO SV,INFA_REP_INFO RP
 WHERE RP.ID = INFA.REP_ID
   AND RP.SERVER_ID = SV.ID
   AND RP.LOCALE = SV.LOCALE
   AND RP.LOCALE = INFA.LOCALE
   AND INFA.LOCALE = :locale
   AND INFA.CAT_ID = :catId
   AND EXISTS
 (SELECT 1
          FROM INFA_USER_WORKFLOW_V V
         WHERE V.USER_ID = :userId
           AND V.WORKFLOW_ID = INFA.ID)
ORDER BY INFA.IDX]]>
  </SQLQuery>
