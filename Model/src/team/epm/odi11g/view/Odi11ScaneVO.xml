<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="Odi11ScaneVO"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="team.epm.odi11g.view.Odi11ScaneVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="locale"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
    <TransientExpression><![CDATA[adf.context.locale]]></TransientExpression>
  </Variable>
  <Variable
    Name="userId"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[adf.userSession.userData.userId]]></TransientExpression>
  </Variable>
  <SQLQuery>
    <![CDATA[SELECT ODI11SCENE.AGENT_ID,
       ODI11SCENE.CAT_ID,
       ODI11SCENE.CREATED_AT,
       ODI11SCENE.CREATED_BY,
       ODI11SCENE.ID,
       ODI11SCENE.LOCALE,
       ODI11SCENE.SCENE_ALIAS,
       ODI11SCENE.SCENE_CONTEXT,
       ODI11SCENE.SCENE_NAME,
       ODI11SCENE.SCENE_VERSION,
       ODI11SCENE.UPDATED_AT,
       ODI11SCENE.UPDATED_BY,
       ODI11SCENE.WORKREP_ID
  FROM ODI11_SCENE ODI11SCENE
 WHERE ODI11SCENE.LOCALE = :locale
   AND EXISTS
 (SELECT 1
          FROM ODI11_USER_SCENE_V V
         WHERE V.USER_ID = :userId
           AND V.SCENE_ID = ODI11SCENE.ID)]]>
  </SQLQuery>
  <EntityUsage
    Name="Odi11Scene"
    Entity="team.epm.odi11g.model.Odi11Scene"/>
  <ViewAttribute
    Name="AgentId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="AgentId"
    EntityUsage="Odi11Scene"
    AliasName="AGENT_ID"/>
  <ViewAttribute
    Name="CatId"
    PrecisionRule="true"
    EntityAttrName="CatId"
    EntityUsage="Odi11Scene"
    AliasName="CAT_ID"/>
  <ViewAttribute
    Name="CreatedAt"
    PrecisionRule="true"
    EntityAttrName="CreatedAt"
    EntityUsage="Odi11Scene"
    AliasName="CREATED_AT"/>
  <ViewAttribute
    Name="CreatedBy"
    IsUpdateable="while_insert"
    PrecisionRule="true"
    EntityAttrName="CreatedBy"
    EntityUsage="Odi11Scene"
    AliasName="CREATED_BY"/>
  <ViewAttribute
    Name="Id"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="Odi11Scene"
    AliasName="ID"/>
  <ViewAttribute
    Name="Locale"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Locale"
    EntityUsage="Odi11Scene"
    AliasName="LOCALE"/>
  <ViewAttribute
    Name="SceneAlias"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="SceneAlias"
    EntityUsage="Odi11Scene"
    AliasName="SCENE_ALIAS"/>
  <ViewAttribute
    Name="SceneContext"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="SceneContext"
    EntityUsage="Odi11Scene"
    AliasName="SCENE_CONTEXT"/>
  <ViewAttribute
    Name="SceneName"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="SceneName"
    EntityUsage="Odi11Scene"
    AliasName="SCENE_NAME"/>
  <ViewAttribute
    Name="SceneVersion"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="SceneVersion"
    EntityUsage="Odi11Scene"
    AliasName="SCENE_VERSION"/>
  <ViewAttribute
    Name="UpdatedAt"
    PrecisionRule="true"
    EntityAttrName="UpdatedAt"
    EntityUsage="Odi11Scene"
    AliasName="UPDATED_AT"/>
  <ViewAttribute
    Name="UpdatedBy"
    PrecisionRule="true"
    EntityAttrName="UpdatedBy"
    EntityUsage="Odi11Scene"
    AliasName="UPDATED_BY"/>
  <ViewAttribute
